@model IEnumerable<MvcBookStore.Models.Genre>

@{
    ViewBag.Title = "Store";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <h3>Kategorie</h3>
    <p>
        Wybierz spośród @Model.Count()
        kategorii:
    </p>

        <ul id="book-list">
            @foreach (var genre in Model)
        {
            <li>
                @Html.ActionLink(genre.Name,
                 "Browse", new { genre = genre.Name })
            </li>

    }
        </ul>



